# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Collections
GET		/collections				controllers.ArchiveItCollections.list()
GET		/collections/save			controllers.ArchiveItCollections.save()
GET		/collections/:id			controllers.ArchiveItCollections.details(id: Long)
#POST	/collections				controllers.ArchiveItCollections.save()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
